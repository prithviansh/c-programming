#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

        int m1, m2, m3, m4, m5;
    int total;
    float average, percentage;

    // Read 5 space-separated integers
    scanf("%d %d %d %d %d", &m1, &m2, &m3, &m4, &m5);

    // Calculate total
    total = m1 + m2 + m3 + m4 + m5;

    // Calculate average
    average = total / 5.0;

    // Calculate percentage (total out of 500)
    percentage = (total / 500.0) * 100;

    // Print the results
    printf("%d %.2f %.2f\n", total, average, percentage);   
    return 0;
}
