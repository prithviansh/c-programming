#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char c;
    int i;
    float f;
    double d;

    // Reading input values
    scanf("%c %d %f %lf", &c, &i, &f, &d);

    // Displaying the values with specified formatting
    printf("%c %d %.2f %.6lf\n", c, i, f, d);
  
    return 0;
}
