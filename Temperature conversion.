#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    float celsius, fahrenheit;

    // Read input
    scanf("%f", &celsius);

    // Convert Celsius to Fahrenheit
    fahrenheit = (celsius * 9 / 5) + 32;

    // Output with 2 decimal places
    printf("%.2f\n", fahrenheit);
   
    return 0;
}
